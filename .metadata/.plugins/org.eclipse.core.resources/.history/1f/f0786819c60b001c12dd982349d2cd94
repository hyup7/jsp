import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		int i=sc.nextInt();//테스트 횟수


		for(int b=0; b<i;b++) {
			int x=sc.nextInt();
			int y=sc.nextInt();
			System.out.println("Case #"+i+":"+(x+y));
		}
	}
	
}
