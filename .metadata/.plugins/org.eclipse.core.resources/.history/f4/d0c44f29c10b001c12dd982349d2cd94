import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		if(y-45<0) {
		x=x-1; 	
		y=y+60-45;
		}
		
		
		if(x<0) {
			x=23;
		}
		System.out.println(x+" "+y);

	}
}
